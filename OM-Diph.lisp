;;;;============================================================================;;;; OM-Diph;;;; Control of Diphone scripts from OM;;;; By Jean Lochard;;;;============================================================================ (defpackage diph   (:use "COMMON-LISP" "OpenMusic" "OM" "OA" "CL-USER"))(in-package diph)(compile&load (make-pathname :directory (append (pathname-directory *load-pathname*) (list "sources")) :name "diph-preferences"))(compile&load (make-pathname :directory (append (pathname-directory *load-pathname*) (list "sources")) :name "OM-Diph-src"));--------------------------------------------------; OM subpackages initialization; ("sub-pack-name" subpacke-lists class-list function-list class-alias-list);--------------------------------------------------(defvar *subpackages-list* nil)(setf *subpackages-list*      '( ("Diphone Script" nil nil (DiphSave DiphSaveAs DiphOpendico DiphCreateDico DiphOpenSequence DiphSaveDescription DiphCreateSeq DiphCopyto DiphSegmentSet DiphSegmentAdd DiphSegmentMul DiphSegmentRatio DiphSegmentGlue DiphSegmentCoorMode DiphSegmentShape DiphSegmentArticul) nil)         ("Diphone Macro" nil nil (DiphBuildSequence DiphBuildRythme DiphSeq&Rythme DiphTimeStretch) nil)         ("Diphone Utilities" nil (Diph-Script) (DiphNameSeg GetDescription DiphExtract DiphPack DiphSequenceLength sdif2part-list WriteDiphScript WriteDiphScript2 ExecuteDiphScript WriteAppleScript WriteAppleScript2 DoSynthesis) nil)         ));--------------------------------------------------;filling packages;--------------------------------------------------(om::fill-library *subpackages-list*)(unless (fboundp 'om::set-lib-release) (defmethod om::set-lib-release (version &optional lib) nil))(om::set-lib-release 1.5)